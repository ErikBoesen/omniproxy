{
	"manifest_version": 2,
	"name": "OmniProxy",
	"version": "0.9.1",
	"description": "Use proxies in Google Chrome from the search bar",
	"options_page": "options.html",
	"omnibox": {
		"keyword": "omni"
	},
	"content_scripts": [{
		"matches": [
			"*://translate.google.com/*",
			"*://www.proxyweb.net/*",
			"*://webcache.googleusercontent.com/*"
		],
		"css": ["proxy.css"],
		"run_at": "document_start"
	}],
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
